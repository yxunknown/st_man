<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.StudentSignCourse" table="student_sign_course" schema="st_man">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="increment"/>
        </id>
        <property name="student">
            <column name="student" sql-type="text"/>
        </property>
        <property name="course">
            <column name="course" sql-type="text"/>
        </property>
        <property name="time">
            <column name="time" sql-type="text"/>
        </property>
    </class>
</hibernate-mapping>